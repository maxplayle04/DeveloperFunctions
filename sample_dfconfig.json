{

  "PLEASE_READ_ME_RIGHT_NOW_ON_GITHUB_THANK_YOU": "Please DO NOT use this as your configuration file, as I have likely added more content."

  "DeveloperFunctionsConfiguration": {

		"DeveloperFunctionsRuntime": {
			"runtimeDebuggingAssistance": true, 
			"signature":  "/this-works/", 
			"moreConsoleData": false
		},

		// Error Management related settings
		"ErrorManagement": {
			"useLegacyErrorManagement":  true 
		},

		
		"Application": {
			"debugOrDeveloperEnabled": true 
		},
		
		
		"Console": {
			
			"WriteProcessInformation": {
				"TimeInTransmission": true, 
				"fallbackSource": "Information" 
				},
			
			"ProcessInformation": {
				"defaultForeColor": 14, 
				"fallbackSource": "Information" 
			},
			
			"PrintStandoutLine": {
				"lineLength": 120, 
				"foreColor": 12, 
				"backColor": 15, 
				"character": "#" 
			},
		
			"NoteToDeveloper": {
				"foreColor": 14, 
				"backColor": 12, 
				"alwaysSendToStorage": true
			}
		},
		
		"FileService": {
			
			"deleteFile":{
				"promptForConfirmation": true 
			}
		}
	}	
}
